{
	"_version": "1.1.0",

	"sap.app": {
		"_version": "1.1.0",
		"id": "sap.training.exc15.components.carriers",
		"type": "component",
		"i18n": "i18n/i18n.properties",
		"title": "{{compTitle}}",
		"description": "{{compDescription}}",
		"applicationVersion": {
			"version": "0.1.0"
		},
		"dataSources": {
			"TravelService": {
				"uri": "/sap/opu/odata/sap/ZBC_TRAVEL_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},

	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "sap.training.exc15.components.carriers.view.Main",

		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {
					"minVersion": "1.30"
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.training.exc15.components.carriers.i18n.i18n"
				}
			},
			"": {
				"dataSource": "TravelService",
				"settings": {}
			}
		}
	}
}